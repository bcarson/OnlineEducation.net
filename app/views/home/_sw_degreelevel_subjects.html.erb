 <div class="module">
	<div class="header">
		<h4>Other Online <%= @subject.name %> Degrees</h4>
	</div>
	<div class="content">
		<ul class="link_list">
			<% for d in @subject.degreelevels %>
			  <%# if Worker.is_degreelevel_subject(d, @subject) %>
				  <li><a href="/online-<%= Worker.inurl(d.name) -%>-degree-in-<%= Worker.inurl(@subject.name) -%>">Online <%= d.name %> <%= @cousin_name == 'Certifications' ? 'Degree' : @cousin_name %> in <%= @subject.name %> </a></li>						
				<%# end %>
      <% end %>
		</ul>
	</div>
	<div class="footer"></div>
</div>
<!-- end .module -->