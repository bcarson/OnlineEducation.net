<div class="module">
	<div class="header">
		<h4>Featured Online Courses</h4>
	</div>
	<div class="content">
		<ul class="link_list">
			<% for program in Program.top_online_programs(@subject) %>
			  <% course = program.courses.first %>
			  <% if Worker.is_not_blank(course.name) %>
			    <li><%= course.name %> in <a rel="nofollow" href="<%= program.getinfourl -%>"> <%= program.name %></a></li>
			  <% end %>
			<% end %>
		</ul>
	</div>
	<div class="footer"></div>
</div>
<!-- end .module -->